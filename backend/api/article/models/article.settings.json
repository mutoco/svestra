{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "name": "Article",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "lead": {
      "type": "text"
    },
    "richtext": {
      "type": "richtext"
    },
    "media": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "private": false,
      "required": true
    },
    "author": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "articles"
    },
    "components": {
      "type": "dynamiczone",
      "components": [
        "teaser.teaser-media"
      ]
    }
  }
}
